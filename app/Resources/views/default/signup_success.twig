{% extends 'base.html.twig' %}
{% block body %}
  <section class="main-block practitioner-bg flex-center">
    <div class="shadow opacity7"></div>
    <div class="main-content flex-spb col-wh">
        <picture>
          <source media="(min-width: 580px)" srcset="{{ asset('images/workinzen-logo-short.png') }}">
          <source srcset="{{ asset('images/workinzen-logo-short-sm.png') }}">
          <img class="short-logo" src="{{ asset('images/workinzen-logo-short.png') }}" alt="Workinzen logo">
        </picture>
      <h1>Votre inscription</h1>
      <h2 class="chapo col-dwh">
          {% for message in app.flashes('signup_success') %}
              {{ message }}
          {% endfor %}
      </h2>
      <hr>
      <h2>Partager</h2>
      <div class="addthis_inline_share_toolbox"></div>
    </div>
  </section>
{% endblock %}
